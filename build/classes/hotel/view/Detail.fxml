            <TextField fx:id="date" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button layoutX="252.0" layoutY="326.0" mnemonicParsing="false" onMouseClicked="#toMenuDisplay" prefHeight="42.0" prefWidth="94.0" text="Save">
         <font>
            <Font name="Arial Bold" size="19.0" />
         </font></Button>
      <Button layoutX="100.0" layoutY="326.0" mnemonicParsing="false" onMouseClicked="#cancel" prefHeight="42.0" prefWidth="94.0" text="Cancel">
         <font>
            <Font name="Arial Bold" size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
